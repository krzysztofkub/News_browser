<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane fx:id="article" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ArticleScene">
   <children>
      <BorderPane prefHeight="719.0" prefWidth="1299.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <WebView fx:id="webView" fontScale="1.7" prefHeight="656.0" prefWidth="1279.0" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <Button fx:id="button" mnemonicParsing="false" onAction="#buttonOnClick" stylesheets="@styles.css" text="Wróć" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </left>
         <top>
            <TextField fx:id="title" editable="false" prefHeight="39.0" prefWidth="475.0" styleClass="article" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </TextField>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
